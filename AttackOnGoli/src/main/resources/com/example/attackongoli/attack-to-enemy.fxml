<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:id="pane" prefHeight="498.0" prefWidth="730.0" xmlns="http://javafx.com/javafx/19"
            xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.attackongoli.AttackToEnemyController">
   <ImageView fitHeight="581.0" fitWidth="746.0" pickOnBounds="true" preserveRatio="true">
      <image>
         <Image url="@MapArena.jpg"/>
      </image>
   </ImageView>
   <Button fx:id="arnoButton" layoutX="14.0" layoutY="24.0" mnemonicParsing="false" onAction="#onArno" prefHeight="64.0"
           prefWidth="73.0" style="-fx-background-color: silver; -fx-border-color: black; -fx-border-radius: 3;">
      <graphic>
         <ImageView fitHeight="56.0" fitWidth="54.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@Arno.png"/>
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button fx:id="jacobButton" layoutX="12.0" layoutY="260.0" mnemonicParsing="false" onAction="#onJacob"
           prefHeight="74.0" prefWidth="77.0"
           style="-fx-background-color: silver; -fx-border-color: black; -fx-border-radius: 3;">
      <graphic>
         <ImageView fitHeight="45.0" fitWidth="58.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@Attacking_Cyndicate.png"/>
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button fx:id="connorButton" layoutX="12.0" layoutY="174.0" mnemonicParsing="false" onAction="#onConnor"
           prefHeight="74.0" prefWidth="73.0"
           style="-fx-background-color: silver; -fx-border-color: black; -fx-border-radius: 3;">
      <graphic>
         <ImageView fitHeight="71.0" fitWidth="61.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@Attacking_Conner.png"/>
            </image>
         </ImageView>
      </graphic>
   </Button>
   <Button fx:id="bearButton" layoutX="14.0" layoutY="101.0" mnemonicParsing="false" onAction="#onBear"
           prefHeight="60.0" prefWidth="50.0"
           style="-fx-background-color: silver; -fx-border-color: black; -fx-border-radius: 3;">
      <graphic>
         <ImageView fitHeight="57.0" fitWidth="63.0" pickOnBounds="true" preserveRatio="true">
            <image>
               <Image url="@Attacking_bear.png"/>
            </image>
         </ImageView>
      </graphic>
   </Button>
</AnchorPane>
